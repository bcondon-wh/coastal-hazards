<div id="application-slide-items-container" class="application-slide-container">
    <div id="application-slide-items-tab" class="application-slide-tab">
        <i class="fa fa-bars"></i>
    </div>
    <div id="application-slide-items-content" class="application-slide-content">
        <div id="application-slide-items-content-container"></div>
    </div>
</div>

<%-- This element is used as a template for creating new items cards --%>
<div id="application-slide-items-container-card-template" class="hidden">
    <div class="application-slide-items-container-card well well-small">
        <div class="row-fluid">
            <div class="span2"><img class="application-slide-items-container-card-image" /></div>
            <div class="application-slide-items-container-card-title-description-container span10">
                <div class="row-fluid">
                    <div class="application-slide-items-container-card-title"><p class="center"></p></div>
                    <div class="application-slide-items-container-card-description"></div>
                </div>
            </div>
        </div>
        <div class="row-fluid text-center">
            <div class="btn-group">
                <button class="application-slide-items-container-card-button-items btn btn-mini application-slide-items-container-card-template-button" type="button"><i class="fa fa-star"></i></button>
                <button class="application-slide-items-container-card-button-remove btn btn-mini application-slide-items-container-card-template-button" type="button"><i class="icon-remove"></i></button>
                <button class="application-slide-items-container-card-button-info btn btn-mini application-slide-items-container-card-template-button" type="button"><i class="fa fa-info-circle"></i></button>
                <button class="application-slide-items-container-card-button-ok btn btn-mini application-slide-items-container-card-template-button" type="button"><i class="fa fa-check"></i></button>
            </div>
        </div>
    </div>
</div>